<p id="notice"><%= notice %></p>

<h1>Listing Comment18s</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Post</th>
      <th>Something</th>
      <th>Somethingelse</th>
      <th>Something more</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comment18s.each do |comment18| %>
      <tr>
        <td><%= comment18.content %></td>
        <td><%= comment18.post %></td>
        <td><%= comment18.something %></td>
        <td><%= comment18.somethingelse %></td>
        <td><%= comment18.something_more %></td>
        <td><%= link_to 'Show', comment18 %></td>
        <td><%= link_to 'Edit', edit_comment18_path(comment18) %></td>
        <td><%= link_to 'Destroy', comment18, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comment18', new_comment18_path %>
