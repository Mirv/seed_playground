<p id="notice"><%= notice %></p>

<h1>Listing Comment3s</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Post</th>
      <th>Something</th>
      <th>Somethingelse</th>
      <th>Something</th>
      <th>More</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comment3s.each do |comment3| %>
      <tr>
        <td><%= comment3.content %></td>
        <td><%= comment3.post %></td>
        <td><%= comment3.something %></td>
        <td><%= comment3.somethingelse %></td>
        <td><%= comment3.something %></td>
        <td><%= comment3.more %></td>
        <td><%= link_to 'Show', comment3 %></td>
        <td><%= link_to 'Edit', edit_comment3_path(comment3) %></td>
        <td><%= link_to 'Destroy', comment3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comment3', new_comment3_path %>
