<p id="notice"><%= notice %></p>

<h1>Listing Comment5s</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Post</th>
      <th>Something</th>
      <th>Somethingelse</th>
      <th>Something</th>
      <th>More</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comment5s.each do |comment5| %>
      <tr>
        <td><%= comment5.content %></td>
        <td><%= comment5.post %></td>
        <td><%= comment5.something %></td>
        <td><%= comment5.somethingelse %></td>
        <td><%= comment5.something %></td>
        <td><%= comment5.more %></td>
        <td><%= link_to 'Show', comment5 %></td>
        <td><%= link_to 'Edit', edit_comment5_path(comment5) %></td>
        <td><%= link_to 'Destroy', comment5, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

